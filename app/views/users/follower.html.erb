

  <h2>フォロワー</h2>
    <div class ="col-xs-12">
      <table class="table">

        <tbody>
          <% @users.each do |user| %>
              <tr>
                <td>  <%= user.user_name %> </td>
                <td>フォロー数：<%= user.followings.count %></td>
                <td>フォロワー数：<%= user.followers.count %></td>
                <td><%= render 'relationships/follow', user: user %></td>
                <td><%= link_to 'Show', user_path(user) %></td>
              </tr>
          <% end %>
        </tbody>
      </table>
    </div>
